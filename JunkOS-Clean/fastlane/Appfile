# Appfile for Umuve

# Your Apple ID email
apple_id(ENV["FASTLANE_APPLE_ID"] || "your@email.com")

# Your Team ID (find at https://developer.apple.com/account)
team_id(ENV["FASTLANE_TEAM_ID"] || "YOUR_TEAM_ID")

# iTunes Connect Team ID (usually same as above)
itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"] || ENV["FASTLANE_TEAM_ID"])

# App bundle identifier
app_identifier("com.goumuve.app")

# For more information about the Appfile, see:
#     https://docs.fastlane.tools/advanced/#appfile
