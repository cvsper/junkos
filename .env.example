# Umuve Environment Configuration
# Copy this file to .env and update with your values

# Flask Configuration
FLASK_ENV=production
SECRET_KEY=change-me-to-a-random-secure-key-in-production

# Database Configuration
POSTGRES_DB=umuve
POSTGRES_USER=umuve
POSTGRES_PASSWORD=umuve_dev_pass

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# React App Configuration
REACT_APP_API_URL=http://localhost/api

# Optional: Override ports
# POSTGRES_PORT=5432
# REDIS_PORT=6379
# BACKEND_PORT=5000
# FRONTEND_PORT=3000
# DASHBOARD_PORT=3001
# NGINX_PORT=80
